Clock_SetID:
SET [Clock_ID], A
SET PC, POP
Clock_SetTicksec:
SET PUSH, B
SET B, A
SET A, CLOCK_SETTICK
HWI [Clock_ID]
SET A, B
SET B, POP
SET PC, POP
Clock_GetTicksec:
SET PUSH, C
SET A, CLOCK_GETTICK
HWI [Clock_ID]
SET A, C
SET C, POP
SET PC, POP
Clock_SetIntMsg:
SET PUSH, B
SET B, A
SET A, CLOCK_TOGLINT
HWI [Clock_ID]
SET A, B
SET B, POP
SET PC, POP
Clock_ID:
DAT 0xFFFF
#define CLOCK_SETTICK		0
#define CLOCK_GETTICK		1
#define CLOCK_TOGLINT		2